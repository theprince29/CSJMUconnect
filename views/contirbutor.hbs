    // Example of fetching data (dummy data)
    const contributors = [
      { name: "Prince Pal", contributions: 180 },
      { name: "Sujit Kumar", contributions: 170 },
      { name: "Shubham Dwivedi", contributions: 170 },
      { name: "Jane Smith", contributions: 120 },
      // Add more contributors here
    ];

    // Function to render contributors in the table
    function renderContributors() {
      const contributorList = document.getElementById("contributorList");
      contributorList.innerHTML = "";

      contributors.forEach((contributor, index) => {
        const row = `
            <tr>
              <td>${index + 1}</td>
              <td>${contributor.name}</td>
              <td>${contributor.contributions}</td>
            </tr>
          `;
        contributorList.innerHTML += row;
      });
    }

    // Initial rendering of contributors
    renderContributors();
  </script>
